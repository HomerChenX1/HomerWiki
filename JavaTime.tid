```java
long timestamp = -1;
SimpleDateFormat dateFormat = new SimpleDateFormat("yyyy MM-dd");
s = dateFormat.format(TrueTime.now().getTime()) + " " +s;

dateFormat = new SimpleDateFormat("yyyy MM-dd HH:mm");
try {
    Date d = dateFormat.parse(s);
    timestamp = d.getTime();
} catch (ParseException e) {
    e.printStackTrace();
}
